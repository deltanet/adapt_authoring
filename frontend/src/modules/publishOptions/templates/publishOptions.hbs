<div class="publishOptions">
  <form class="frameworkImport forms" action="importsourcecheck" method="post" autocomplete="off">
  <div class="inner publish-summary" id="summary-list">
      <div class="title green">Publish settings</div>
      <div class="description">The course will be published with the following settings.</div>
      <div class="col-row tb-row publish-summary-none">
        <div class="col-100">{{t 'app.noplugintypes'}}</div>
      </div>
  </div>
  <div class="inner publish-options" id="options-list">
    <div class="title amber">Publish options</div>
    <div class="description">The following plugins can be used.</div>
    <div class="col-row tb-row publish-options-none">
      <div class="col-100">{{t 'app.noplugintypes'}}</div>
    </div>
  </div>
  <div class="inner error-summary">
      <div class="title red">Errors</div>
      <div class="description">The following errors have occured.</div>
      <div class="col-row tb-row">
        <div class="col-100"><strong>errors</strong></div>
      </div>
  </div>
  </form>
</div>
